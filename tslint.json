{
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "no-magic-numbers": true,
    "prefer-for-of": true,
    "radix": true
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}